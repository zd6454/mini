<view class="boxWrap" wx:for="{{data_list}}">
  <image class="img" src="{{item.image}}"></image>
  <view class="txtWrap">
    <view class="text"> {{item.text}}</view>
  </view>
</view>
<block>
  <view wx:if="{{htmlData}}">
  <rich-text nodes="{{htmlData}}"></rich-text>
  <view>{{htmlData}}</view>
</view>
<view class="null" wx:else>请等待管理员发布。</view>
</block>