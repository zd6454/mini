<!--pages/my/index.wxml-->
<view class="page">
  <view class="head">
    <view class="avatar">
      <image src="../../icons/alumn1.png"  class="imageset" />
    </view>
    <view class="info">
      <view class="name"> 1234</view>
      <view class="base">
        <view class="gender">男</view>
        <view class="level">大三</view>
      </view>
      <view class="school">武汉理工大学</view>
    </view>
  </view>
  <view class="body">
    <view class="tabs" wx:for="{{tabs}}" wx:key="item" data-set="{{item}}" wx:for-item="item" wx:for-index="index" >
      <view class="tab">
        <image src="{{item.icon}}"  class="tabIcon"   />
        <view>{{item.name}}</view>
      </view>
    </view>
  </view>
  <view class="foot" >
    <view class="setting"  wx:for="{{setting}}" wx:key="item" data-set="{{item}}" wx:for-item="item" wx:for-index="index"  bindtap="todetail">
      <view class="detail">{{item.name}}</view>
      <view class="link">></view>
    </view> 
  </view>
  <view class="loginOut">
    <view>退出登录</view>
    <view>></view>
  </view>
</view>
