
<view class="page">
  <view class="head" wx:if="{{key=='schoolmateId'}}">
   <view class="avatar">
     <image class="img" src="{{data.imgUrl}}"></image>
   </view>
   <view class="name">{{data.name}}</view>
  </view>
  <view wx:if="{{data.content}}">
  <rich-text class="text" nodes="{{data.content}}"></rich-text>
  </view>
  <view class="null" wx:else>请等待管理员发布。</view>
</view>
