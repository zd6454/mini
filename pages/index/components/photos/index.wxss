.photo{
  margin-top: 20rpx;
  width: 96%;
  margin: auto;
}
.photo .rowWrap{
  width: 100%;
  height: 500rpx;
  display: flex;
  flex-direction: row;
}
.photo .rowWrap .leftWrap{
  width: 50%;
  display: flex;
  flex-direction: column;
}
.photo .rowWrap .leftWrap .topWrap{
  width: 96%;
  height: 67%;
  margin: 10rpx;
  border-radius: 10rpx;
}
.photo .rowWrap .leftWrap .bottomWrap{
  width: 100%;
  height: 33%;
  display: flex;
  flex-direction: row;
  margin: 0 10rpx;
  /* margin: 10rpx; */
}
.photo .rowWrap .leftWrap .bottomWrap .bottomL{
  width: 47%;
  height: 100%;
  border-radius: 10rpx;

}