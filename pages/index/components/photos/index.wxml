<view class="photo">
  <view class="rowWrap" wx:for="{{photoList}}">
  <view class="leftWrap">
    <image class="topWrap" src="{{item.imgUrl}}" bindtap="handlePreview" data-src="{{item.imgUrl}}"></image>
    <view class="bottomWrap" >
      <image class="bottomL" src="{{item.imgUrl}}" style="margin-right:10rpx" bindtap="handlePreview" data-src="{{item.imgUrl}}"></image>
      <image class="bottomL" src="{{item.imgUrl}}" bindtap="handlePreview" data-src="{{item.imgUrl}}"></image>
    </view>
  </view>
  <view class="leftWrap" style="flex-direction: column-reverse;margin-top: 10rpx">
    <image class="topWrap" src="{{item.imgUrl}}" style="margin-bottom:0" bindtap="handlePreview" data-src="{{item.imgUrl}}"></image>
    <view class="bottomWrap" >
      <image class="bottomL" src="{{item.imgUrl}}" style="margin-right:10rpx" bindtap="handlePreview" data-src="{{item.imgUrl}}"></image>
      <image class="bottomL" src="{{item.imgUrl}}" bindtap="handlePreview" data-src="{{item.imgUrl}}"></image>
    </view>
  </view>
</view>
</view>
